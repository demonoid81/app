  "name": "app",
  "version": "1.0.0",
  "description": "fullstack app",
  "main": "index.js",
  "scripts": {
    "server": "nodemon --watch ./server ./server/index.js",
    "prepush": "npm run ilint -q",
    "dll": "npx webpack --config ./webpack.dll.config.js",
    "postinstall": "npm -s run dll",
    "dev": "npx cross-env NODE_ENV=development node --trace-deprecation ./build/dev-server.js",
    "build": "npx rimraf dist && npx cross-env NODE_ENV=production npx webpack --config ./build/webpack.prod.config.js --hide-modules",
    "ilint": "npx eslint client/**/*.js client/**/*.vue",
    "fix": "npx eslint --fix client/**/*.js client/**/*.vue",
    "commitmsg": "npx commitlint -e",
    "devserver": "npx cross-env NODE_ENV=development node build/dev-server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/demonoid81/app.git"
